{
    "name": "wplibs/form",
    "description": "A WordPress Form component based on the CMB2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Van Anh",
            "email": "anhskohbo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.8",
        "cmb2/cmb2": "^2.5",
        "wplibs/http": "^1.0",
        "wplibs/rules": "^1.0",
        "tightenco/collect": "5.2.*|5.3.*|5.4.*",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*|5.7.*",
        "mockery/mockery": "^0.9|^1.0"
    },
    "autoload": {
        "psr-4": {
            "WPLibs\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WPLibs\\Form\\Test\\": "tests/"
        }
    },
    "prefer-stable" : true,
    "minimum-stability": "dev"
}
